{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "jquery": true,
    "jest": true
  },
  "rules": {
    "no-use-before-define": 0,
    "no-console": 0,
    "max-len": [1, 180],
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "no-mixed-operators": 0,
    "prefer-spread": 0
  }
}